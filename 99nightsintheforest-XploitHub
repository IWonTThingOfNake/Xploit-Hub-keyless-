local library = loadstring(game:HttpGet('https://raw.githubusercontent.com/inceldom/kinx/main/ui'))()
local win = library:Window("99 nights in the forest | V1.0 | Xploit Hub",Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)
local Items = win:Tab("Items")
local Player = win:Tab("Player")

local client = game.Players.LocalPlayer

-- plr

Player:Slider("WalkSpeed",0,1000,16, function(t)
	client.Character.Humanoid.WalkSpeed = t
end)

Player:Slider("JumpPower",0,1000,50, function(t)
	client.Character.Humanoid.JumpPower = t
end)

local infJump = false

Player:Toggle("Infinite Jump",false, function(t)
	infJump = t
end)

-- items

Items:Button("Store Logs - Old Sack", function()
    for i = 1,5 do
        local log = workspace.Items:FindFirstChild("Log")
        client.Character.HumanoidRootPart.CFrame = log.Main.CFrame + Vector3.new(0,5,0)
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestBagStoreItem"):InvokeServer(client.Inventory["Old Sack"], log)
    end
end)

Items:Button("Store Logs - Good Sack", function()
    for i = 1,15 do
        local log = workspace.Items:FindFirstChild("Log")
        client.Character.HumanoidRootPart.CFrame = log.Main.CFrame + Vector3.new(0,5,0)
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestBagStoreItem"):InvokeServer(client.Inventory["Good Sack"], log)
    end
end)

game:GetService("UserInputService").JumpRequest:Connect(function()
    if infJump then
		client.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	end
end)
